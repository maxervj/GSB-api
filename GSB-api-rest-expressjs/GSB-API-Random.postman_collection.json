								"exec": [
									"// Génération de données aléatoires pour un motif",
									"const motifs = [",
									"    'Présentation de nouveaux produits',",
									"    'Formation professionnelle',",
									"    'Suivi client régulier',",
									"    'Démonstration technique',",
									"    'Conférence médicale',",
									"    'Visite de courtoisie',",
									"    'Présentation de résultats d\\'études',",
									"    'Information sur nouvelles thérapies',",
									"    'Visite de prospection',",
									"    'Remise d\\'échantillons',",
									"    'Discussion protocole thérapeutique',",
									"    'Réunion de service',",
									"    'Présentation de documentation',",
									"    'Enquête de satisfaction',",
									"    'Présentation campagne publicitaire',",
									"    'Mise à jour catalogue produits',",
									"    'Présentation nouveaux traitements',",
									"    'Visite post-formation',",
									"    'Suivi commercial',",
									"    'Présentation matériel médical'",
									"];",
									"",
									"// Génération aléatoire avec numéro unique",
									"const randomMotif = motifs[Math.floor(Math.random() * motifs.length)];",
