					"name": "Créer un praticien (Aléatoire)",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Génération de données aléatoires pour un praticien",
									"const noms = ['Martin', 'Dubois', 'Lefevre', 'Mercier', 'Rousseau', 'Blanc', 'Guerin', 'Muller', 'Henry', 'Girard', 'Andre', 'Roux', 'Lambert', 'Bonnet', 'Faure', 'Perrin', 'Clement', 'Gauthier', 'Dufour', 'Robin'];",
									"const prenoms = ['Sophie', 'Marc', 'Julie', 'Antoine', 'Camille', 'Nicolas', 'Laura', 'Thomas', 'Emma', 'Alexandre', 'Léa', 'Julien', 'Chloé', 'Maxime', 'Sarah', 'David', 'Marion', 'Benjamin', 'Lucie', 'Romain'];",
									"const villes = [",
									"    {nom: 'Paris', cp: '75001'},",
									"    {nom: 'Lyon', cp: '69001'},",
									"    {nom: 'Marseille', cp: '13001'},",
									"    {nom: 'Toulouse', cp: '31000'},",
									"    {nom: 'Nice', cp: '06000'},",
									"    {nom: 'Nantes', cp: '44000'},",
									"    {nom: 'Strasbourg', cp: '67000'},",
									"    {nom: 'Montpellier', cp: '34000'},",
									"    {nom: 'Bordeaux', cp: '33000'},",
									"    {nom: 'Lille', cp: '59000'}",
									"];",
									"const rues = ['Rue de la Santé', 'Avenue Victor Hugo', 'Boulevard Haussmann', 'Rue de la République', 'Place de la Mairie', 'Rue Pasteur', 'Avenue Jean Jaurès', 'Rue Voltaire', 'Boulevard Gambetta', 'Rue des Écoles'];",
									"",
									"// Génération aléatoire",
									"const nom = noms[Math.floor(Math.random() * noms.length)];",
									"const prenom = prenoms[Math.floor(Math.random() * prenoms.length)];",
									"const randomNum = Math.floor(Math.random() * 10000);",
									"const tel = '01' + String(Math.floor(Math.random() * 100000000)).padStart(8, '0');",
									"const email = `dr.${prenom.toLowerCase()}.${nom.toLowerCase()}${randomNum}@medecin.fr`;",
									"const rue = Math.floor(Math.random() * 150 + 1) + ' ' + rues[Math.floor(Math.random() * rues.length)];",
									"const ville = villes[Math.floor(Math.random() * villes.length)];",
									"",
									"// Stockage dans les variables d'environnement",
									"pm.environment.set('praticien_nom', nom);",
									"pm.environment.set('praticien_prenom', prenom);",
									"pm.environment.set('praticien_tel', tel);",
									"pm.environment.set('praticien_email', email);",
									"pm.environment.set('praticien_rue', rue);",
									"pm.environment.set('praticien_code_postal', ville.cp);",
									"pm.environment.set('praticien_ville', ville.nom);",
									"",
									"console.log(`Praticien généré: Dr ${prenom} ${nom} - ${ville.nom} - ${email}`);"
								],
