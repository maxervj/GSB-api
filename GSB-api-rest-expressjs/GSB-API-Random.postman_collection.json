				{
					"name": "Créer un visiteur (Aléatoire)",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Génération de données aléatoires pour un visiteur",
									"const noms = ['Dupont', 'Martin', 'Bernard', 'Thomas', 'Robert', 'Petit', 'Durand', 'Leroy', 'Moreau', 'Simon', 'Laurent', 'Lefebvre', 'Michel', 'Garcia', 'David', 'Bertrand', 'Roux', 'Vincent', 'Fournier', 'Morel'];",
									"const prenoms = ['Jean', 'Marie', 'Pierre', 'Sophie', 'Luc', 'Anne', 'Michel', 'Claire', 'François', 'Isabelle', 'Jacques', 'Nathalie', 'Philippe', 'Sylvie', 'Alain', 'Catherine', 'Christophe', 'Martine', 'Thierry', 'Monique'];",
									"",
									"// Génération aléatoire",
									"const nom = noms[Math.floor(Math.random() * noms.length)];",
									"const prenom = prenoms[Math.floor(Math.random() * prenoms.length)];",
									"const randomNum = Math.floor(Math.random() * 10000);",
									"const tel = '06' + String(Math.floor(Math.random() * 100000000)).padStart(8, '0');",
									"const email = `${prenom.toLowerCase()}.${nom.toLowerCase()}${randomNum}@gsb.fr`;",
									"",
									"// Date d'embauche aléatoire (entre 2020 et 2024)",
									"const startDate = new Date(2020, 0, 1);",
									"const endDate = new Date(2024, 11, 31);",
									"const randomDate = new Date(startDate.getTime() + Math.random() * (endDate.getTime() - startDate.getTime()));",
									"const date_embauche = randomDate.toISOString().split('T')[0];",
									"",
									"// Stockage dans les variables d'environnement",
									"pm.environment.set('visiteur_nom', nom);",
									"pm.environment.set('visiteur_prenom', prenom);",
									"pm.environment.set('visiteur_tel', tel);",
									"pm.environment.set('visiteur_email', email);",
									"pm.environment.set('visiteur_date_embauche', date_embauche);",
									"",
									"console.log(`Visiteur généré: ${prenom} ${nom} - ${email}`);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Sauvegarde automatique de l'ID du visiteur créé",
									"var jsonData = pm.response.json();",
									"if (jsonData.success && jsonData.data._id) {",
									"    pm.environment.set('visiteur_id', jsonData.data._id);",
									"    console.log('✅ Visiteur créé avec ID: ' + jsonData.data._id);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"nom\": \"{{visiteur_nom}}\",\n  \"prenom\": \"{{visiteur_prenom}}\",\n  \"tel\": \"{{visiteur_tel}}\",\n  \"email\": \"{{visiteur_email}}\",\n  \"date_embauche\": \"{{visiteur_date_embauche}}\"\n}"
						},
